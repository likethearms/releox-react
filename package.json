{
  "name": "releox-react",
  "version": "0.1.0-beta.57",
  "typings": "./index.d.ts",
  "main": "lib/index.js",
  "author": "Henrik Salmela",
  "dependencies": {
    "@types/react-select": "^2.0.17",
    "prop-types": "^15.7.2",
    "query-string": "^6.8.3",
    "react-loading": "^2.0.3",
    "react-select": "^2.4.3",
    "webpack": "4.39.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "sass": "sass src/releox.scss:src/releox.css",
    "sass:watch": "npm run sass -- --watch",
    "build": "./scripts/pre-build.sh && npm run sass && mkdir lib && cp src/releox.css lib/releox.css && tsc -p ./build-configs/tsconfig.json && ./scripts/post-build.sh",
    "prepublish": "npm run build",
    "postpublish": "PACKAGE_VERSION=$(cat package.json | grep \\\"version\\\" | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g' | tr -d '[[:space:]]') && git tag v$PACKAGE_VERSION && git push --tags",
    "publish-beta": "npm publish --tag beta",
    "test": "react-scripts test",
    "lint": "eslint src --ext .js,.tsx,.ts",
    "create-typings": "tsc --project tsconfig.typing.json",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx,js,jsx}"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@coreui/coreui": "^2.1.12",
    "@storybook/addon-actions": "^5.1.11",
    "@storybook/addon-info": "^5.1.11",
    "@storybook/addon-links": "^5.1.11",
    "@storybook/react": "^5.1.11",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^24.0.18",
    "@types/moxios": "^0.4.8",
    "@types/node": "^11.12.1",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^4.3.5",
    "@types/storybook__addon-actions": "^3.4.3",
    "@types/storybook__addon-links": "^3.3.5",
    "@types/storybook__react": "^4.0.2",
    "@typescript-eslint/eslint-plugin": "^2.0.0",
    "@typescript-eslint/parser": "^2.0.0",
    "axios": "^0.19.0",
    "bootstrap": "^4.3.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "enzyme-wait": "^1.0.9",
    "eslint": "^6.3.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "formik": "^1.5.8",
    "moxios": "^0.4.0",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "^3.1.1",
    "sass": "^1.22.10",
    "typescript": "^3.5.0"
  }
}
