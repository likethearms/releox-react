{
  "name": "releox-react",
  "version": "2.0.0-beta.3",
  "typings": "lib/index.d.ts",
  "main": "lib/index.js",
  "author": "Henrik Salmela",
  "dependencies": {
    "deepmerge": "^4.2.2",
    "get-value": "^3.0.1",
    "query-string": "^6.8.3",
    "react-bootstrap-table-next": "^3.2.1",
    "react-bootstrap-table2-paginator": "^2.1.0",
    "react-loading": "^2.0.3",
    "react-select": "^3.0.8"
  },
  "scripts": {
    "start": "react-scripts start",
    "sass": "sass src/releox.scss:src/releox.css",
    "sass:watch": "npm run sass -- --watch",
    "build": "./scripts/pre-build.sh && npm run sass && mkdir lib && cp src/releox.css lib/releox.css && tsc -p ./build-configs/tsconfig.json && npm run create-typings && ./scripts/post-build.sh",
    "prepublish": "npm run build",
    "postpublish": "PACKAGE_VERSION=$(cat package.json | grep \\\"version\\\" | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g' | tr -d '[[:space:]]') && git tag v$PACKAGE_VERSION && git push --tags",
    "publish-beta": "npm publish --tag beta",
    "test": "react-scripts test",
    "lint": "eslint src --ext .js,.tsx,.ts",
    "create-typings": "tsc --project ./build-configs/tsconfig.typing.json",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx,js,jsx}",
      "!src/testing-cra/*",
      "!src/stories/*",
      "!src/index.tsx",
      "!src/cra-index.tsx"
    ]
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@coreui/coreui": "^2.1.12",
    "@testing-library/react": "^9.4.0",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/get-value": "^3.0.1",
    "@types/jest": "^24.0.18",
    "@types/moxios": "^0.4.9",
    "@types/node": "^12.7.11",
    "@types/react": "^16.9.4",
    "@types/react-dom": "^16.9.1",
    "@types/react-redux": "^7.1.4",
    "@types/react-router": "^5.1.1",
    "@types/react-router-dom": "^5.1.0",
    "@types/react-router-redux": "^5.0.18",
    "@types/react-select": "^3.0.5",
    "@types/redux-mock-store": "^1.0.1",
    "@types/redux-thunk": "^2.1.0",
    "@types/yup": "^0.26.24",
    "@typescript-eslint/eslint-plugin": "^2.3.2",
    "@typescript-eslint/parser": "^2.3.2",
